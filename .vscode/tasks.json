{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "npm",
      "script": "compile",
      "group": "build",
      "presentation": {
        "panel": "shared",
        "reveal": "silent"
      },
      "problemMatcher": [
        "$tsc",
        "$eslint-stylish"
      ]
    },
    {
      "type": "npm",
      "script": "watch",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "panel": "shared",
        "reveal": "never"
      },
      "problemMatcher": [
        "$tsc-watch",
        "$eslint-stylish"
      ],
      "isBackground": true
    },
    {
      "type": "npm",
      "script": "package",
      "group": "build",
      "presentation": {
        "panel": "shared",
        "reveal": "always"
      },
      "problemMatcher": []
    },
    {
      "type": "npm",
      "script": "test",
      "group": "test",
      "presentation": {
        "panel": "shared",
        "reveal": "always"
      },
      "problemMatcher": []
    }
  ]
}